	</servlet>
	<servlet-mapping>
		<servlet-name>Sign_UpServlet</servlet-name>
		<!-- <url-pattern>/signup_servlet/</url-pattern> -->
		<url-pattern>/signup</url-pattern>
	</servlet-mapping>
	<servlet>
