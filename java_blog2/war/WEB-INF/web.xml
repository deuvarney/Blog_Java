	</servlet>
	<servlet-mapping>
		<servlet-name>Logout_Servlet</servlet-name>
		<url-pattern>/logout</url-pattern>
	</servlet-mapping>

	<servlet>
		<servlet-name>Signup_Page</servlet-name>
